<h1>New Animal for <%= @farm.name %></h1>

<%= form_with url: "/farms/#{@farm.id}/animals", method: :post, local: true do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>

  <%= f.label :age, "Age" %>
  <%= f.text_field :age %>

  <%= f.label :rescue, "Rescue" %>
  <%= f.text_field :rescue %>

  <%= f.submit 'Create New Animal' %>
<% end %>